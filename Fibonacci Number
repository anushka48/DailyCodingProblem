# Dynamic Programming solution that initializes the default/base fibonacci numbers and then builds up the sequence by adding the two previous iteratively.
class Solution:
    def fib(self, n: int) -> int:
        seq = [0,1]
        for i in range(2, n+1):
            seq.append(seq[i-1] + seq[i-2])
        return seq[n]

# Same logic but using recursion:
class Solution:
    def fib(self, n: int) -> int:
        if (n == 0):
            return 0
        elif (n == 1):
            return 1
        else:
            return self.fib(n-1) + self.fib(n-2)
